{
  "name": "picsart",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:compressed": "vite build && serve -s dist",
    "performance": "node performance-test.cjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "analyze": "npm run build && open stats.html",
    "test:perf": "npm run build && npm run preview -- --open",
    "test:lighthouse": "npm run build && npm run preview:compressed & sleep 2 && npx lighthouse http://localhost:3000 --output-path=./report/lighthouse-report.html --view",
    "dev:mock": "cross-env VITE_USE_MOCK_DATA=true vite"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@tanstack/react-query": "^5.69.0",
    "@types/react-lazy-load-image-component": "^1.6.4",
    "@types/styled-components": "^5.1.34",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-lazy-load-image-component": "^1.6.3",
    "react-router-dom": "^7.4.0",
    "styled-components": "^6.1.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-plugin-styled-components": "^2.1.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jsdom": "^26.0.0",
    "lighthouse": "^11.7.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "serve": "^14.2.4",
    "terser": "^5.39.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-compression2": "^1.3.3",
    "vitest": "^3.0.9"
  }
}
